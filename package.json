{
  "name": "@torb.xyz/mine",
  "version": "0.1.7",
  "description": "TypeScript library that allows you to easily get (“mine”) data from Craft CMS.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "test": "jest --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "testRegex": "/src/.*\\.test\\.ts$"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/torb-xyz/mine.git"
  },
  "author": "Torb Lunde Miranda",
  "license": "UNLICENSED",
  "homepage": "https://github.com/torb-xyz/mine#readme",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "jest": "^25.3.0",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
